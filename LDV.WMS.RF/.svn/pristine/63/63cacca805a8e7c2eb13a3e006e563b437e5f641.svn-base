    <select id="SelectPartInfoByCode"  parameterClass="System.Collections.IDictionary">
      <![CDATA[ 
        select l.loc_code, i.*
        from v_wms_item i,
          v_wms_item_location_map m, 
          v_wms_base_location l
        where i.id = m.item_id and m.loc_id = l.id 
