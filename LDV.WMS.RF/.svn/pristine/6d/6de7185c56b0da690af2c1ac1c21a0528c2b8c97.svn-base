    <typeAlias alias="LocItem" type="LDV.WMS.RF.DataMapper.LocItem, LDV.WMS.RF.DataMapper" />    
	</alias>
	
	<resultMaps>
		<resultMap id="SelectResult" class="VWmsInvDetail">
