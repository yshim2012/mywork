			<selectKey resultClass="double" type="pre" property="ID" > 
				<![CDATA[ select V_WMS_PICK_TICKET_DETAIL_SEQ.NEXTVAL as value from dual ]]>
			</selectKey>
			<![CDATA[
			insert into V_WMS_PICK_TICKET_DETAIL
			(
			    ID,
			    PICK_TICKET_ID,
			    LINE_NO,
			    STATUS,
			    ITEM_ID,
			    ORDER_QTY,
