			    SALE_ORDER_LINE_NO,
			    CREATED_TIME,
			    CANCEL_DATE,
			    LAST_MODIFIED,
			    KBNUMBER
			)
			values 
			(
				#ID#,
				#PICK_TICKET_ID#,
				#LINE_NO#,
				#STATUS#,
				#ITEM_ID#,
				#ORDER_QTY#,
				#ORDER_PLAN_QTY#,
