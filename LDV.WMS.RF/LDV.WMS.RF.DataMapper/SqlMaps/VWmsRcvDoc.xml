    <insert id="Insert" parameterClass="VWmsRcvDoc">
      <selectKey resultClass="double" type="pre" property="ID" >
        <![CDATA[ select V_WMS_RCV_DOC_SEQ.NEXTVAL as value from dual ]]>
      </selectKey>
