<Window x:Class="WpfLDVPacakge.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="零件包装标准查询系统V1.0" Height="692" Width="835">
    <Grid Height="649" Width="816">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="12*" />
            <ColumnDefinition Width="676*" />
        </Grid.ColumnDefinitions>
        <Image Height="591" HorizontalAlignment="Left" Margin="4,0,0,12" Name="image1" Stretch="Fill" VerticalAlignment="Bottom" Width="786" ImageFailed="image1_ImageFailed" Grid.Column="1" />
        <Label Content="零件号：" Height="28" HorizontalAlignment="Left" Margin="4,12,0,0" Name="label1" VerticalAlignment="Top" Width="62" Grid.Column="1" />
        <TextBox Height="28" HorizontalAlignment="Left" Margin="72,12,0,0" Name="txtPart" VerticalAlignment="Top" Width="147" Grid.Column="1" KeyDown="txtPart_KeyDown" />
        <Label Content="" Height="28" HorizontalAlignment="Left" Margin="316,10,0,0" Name="lbMassage" VerticalAlignment="Top" Width="474" Grid.Column="1" Foreground="Crimson" />
        <Button Content="确认" Grid.Column="1" Height="23" HorizontalAlignment="Left" Margin="235,13,0,0" Name="butOK" VerticalAlignment="Top" Width="75" Click="butOK_Click" />
    </Grid>
</Window>
