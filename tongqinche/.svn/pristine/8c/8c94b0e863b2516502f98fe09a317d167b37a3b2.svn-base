using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace DataMapper
{
    /// <summary>
    /// 用户表
    /// </summary>
    public class User : BaseEntity<User>
    {
        /// <summary>
        /// 
        /// </summary>
        public int Id
        {
            get;
            set;
        }

        /// <summary>
        /// 昵称
        /// </summary>
        public string User_Name
        {
            get;
            set;
        }

        /// <summary>
        /// 真实姓名
        /// </summary>
        public string Real_Name
        {
            get;
            set;
        }

        /// <summary>
        /// 头像
        /// </summary>
        public string Icon_Url
        {
            get;
            set;
        }

        /// <summary>
        /// 工号
        /// </summary>
        public string Employee_No
        {
            get;
            set;
        }

        /// <summary>
        /// 密码
        /// </summary>
        public string Password
        {
            get;
            set;
        }

        /// <summary>
        /// 部门
        /// </summary>
        public string Department_Name
        {
            get;
            set;
        }

        /// <summary>
        /// 在职公司
        /// </summary>
        public int Company_Name
        {
            get;
            set;
        }

        /// <summary>
        /// 联系电话
        /// </summary>
        public string Tel
        {
            get;
            set;
        }

        /// <summary>
        /// 邮箱
        /// </summary>
        public string Email
        {
            get;
            set;
        }

        /// <summary>
        /// 是否作废
        /// </summary>
        public int Is_Delete
        {
            get;
            set;
        }

        /// <summary>
        /// 作废时间
        /// </summary>
        public Nullable<DateTime> Delete_Date
        {
            get;
            set;
        }

        /// <summary>
        /// 最后登录时间
        /// </summary>
        public Nullable<DateTime> Last_Login_Time
        {
            get;
            set;
        }

        /// <summary>
        /// 创建时间
        /// </summary>
        public Nullable<DateTime> CreateTime
        {
            get;
            set;
        }

        /// <summary>
        /// 创建用户
        /// </summary>
        public int CreateUserId
        {
            get;
            set;
        }

        /// <summary>
        /// 最后更新时间
        /// </summary>
        public Nullable<DateTime> UpdateTime
        {
            get;
            set;
        }

        /// <summary>
        /// 最后更新用户
        /// </summary>
        public int UpdateUserId
        {
            get;
            set;
        }

    }
}
