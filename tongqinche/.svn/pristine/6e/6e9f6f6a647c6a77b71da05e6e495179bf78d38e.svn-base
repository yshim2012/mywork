
@{
    ViewBag.Title = "CarrierCreate";
}

@section Head{
    <link href="../media/css/jqvmap.css" media="screen" rel="stylesheet" type="text/css" />
    }


<div class="container-fluid">
    <!-- BEGIN PAGE HEADER-->

    <div class="row-fluid">

        <div class="span12">

            <ul class="breadcrumb">

                <li>

                    <i class="icon-home"></i>

                    @Html.ActionLink("首页", "Index", "Home", routeValues: null, htmlAttributes: new { id = "IndexLink" })

                    <span class="icon-angle-right"></span>

                </li>

                <li>

                    <a href="#">基础数据管理</a>

                    <span class="icon-angle-right"></span>

                </li>

                <li>
                    @Html.ActionLink("承运商新增", "CarrierCreate", "Carrier", routeValues: null, htmlAttributes: new { id = "CarrierLink" })
                </li>

            </ul>

        </div>

    </div>

    <!-- END PAGE HEADER-->
    <!-- BEGIN PAGE CONTENT-->

    <div class="row-fluid">

        <div class="span12">

            <!-- BEGIN SAMPLE FORM PORTLET-->

            <div class="portlet-body form">

                <div class="tabbable portlet-tabs">

                    <div class="portlet-body">

                        <div class="clearfix">

                            <div class="btn-group">

                                <button id="btnAdd" class="btn green" onclick="btnAdd_Click()">

                                    新增 <i class="icon-save"></i>

                                </button>
                            </div>
                            <div class="btn-group">
                                <button id="btnBack" class="btn green">

                                    返回 <i class="icon-retweet"></i>

                                </button>
                            </div>

                        </div>


                    </div>

                    <br />
                    <br />
                    <br />
                    <div class="tab-content">

                        <div class="tab-pane active" id="portlet_tab1">

                            <!-- BEGIN FORM-->

                            <form action="#" class="form-horizontal">

                                <div class="row-fluid">
                                    <div class="span6 ">
                                        <div class="control-group">

                                            <label class="control-label">承运商名称:</label>

                                            <div class="controls">

                                                <input id="DictCode" name="DictCode" type="text" class="m-wrap Meduam" />

                                            </div>

                                        </div>
                                    </div>
                                    <div class="span6 ">
                                        <div class="control-group">
                                            <label class="control-label">联系人:</label>

                                            <div class="controls">

                                                <input id="DictName" name="DictName" type="text" class="m-wrap Meduam" />
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <div class="row-fluid">

                                    <div class="span6 ">

                                        <div class="control-group">

                                            <label class="control-label">手机:</label>

                                            <div class="controls">
                                                <input id="remark" name="remark" type="text" class="m-wrap Meduam" />
                                            </div>
                                        </div>

                                    </div>
                                    <div class="span6 ">
                                        <div class="control-group">

                                            <label class="control-label">电话:</label>

                                            <div class="controls">

                                                <input id="remark" name="remark" type="text" class="m-wrap Meduam" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row-fluid">

                                    <div class="span6 ">

                                        <div class="control-group">

                                            <label class="control-label">地址:</label>

                                            <div class="controls">
                                                <input id="remark" name="remark" type="text" class="m-wrap Meduam" />
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </form>
                            <!-- END FORM-->
                        </div>
                    </div>
                </div>

            </div>

        </div>

        <!-- END SAMPLE FORM PORTLET-->

    </div>
    <div class="row-fluid">

        <div class="span12">


        </div>
    </div>
    <!-- END PAGE CONTENT-->
</div>

@section Body{
    <script>

    </script>
    }